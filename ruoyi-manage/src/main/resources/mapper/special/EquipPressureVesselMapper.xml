    <sql id="selectEquipPressureVesselVo">
        select id,
               name,
               specification,
               volume,
               equipment_category,
               installation_type,
               design_pressure,
               design_temperature,
               working_pressure,
               working_temperature,
               material,
               medium,
               net_weight,
               manufacturer,
               manufacture_date,
               manufacture_license_no,
               manufacture_serial_no,
               installation_unit,
               installation_date,
               commissioning_date,
               equipment_code,
               usage_certificate_no,
               usage_registration_certificate,
               remarks,
               created_by,
               created_time,
               updated_by,
               updated_time
        from equip_pressure_vessel
