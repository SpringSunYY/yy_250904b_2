    <select id="selectEquipPressureVesselList" parameterType="EquipPressureVessel"
            resultMap="EquipPressureVesselResult">
        <include refid="selectEquipPressureVesselVo"/>
        <where>
            <if test="name != null  and name != ''">and name like concat('%', #{name}, '%')</if>
            <if test="equipmentCategory != null  and equipmentCategory != ''">and equipment_category =
                #{equipmentCategory}
            </if>
            <if test="installationType != null  and installationType != ''">and installation_type =
                #{installationType}
            </if>
        </where>
    </select>

    <select id="selectEquipPressureVesselById" parameterType="Long" resultMap="EquipPressureVesselResult">
        <include refid="selectEquipPressureVesselVo"/>
        where id = #{id}
    </select>

