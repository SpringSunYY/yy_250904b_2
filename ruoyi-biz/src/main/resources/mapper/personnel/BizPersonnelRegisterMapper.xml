    <sql id="selectBizPersonnelRegisterVo">
        select personnel_id, choice_id, project_name, contractor_name, id_card_number, personnel_name, title, gender, age, work_type, contact_phone, home_address, physical_exam_date, physical_exam_result, physical_exam_record, entry_factory_time, exit_factory_time, remarks, created_by, created_at, updated_by, updated_at from biz_personnel_register
    </sql>

    <select id="selectBizPersonnelRegisterList" parameterType="BizPersonnelRegister" resultMap="BizPersonnelRegisterResult">
        <include refid="selectBizPersonnelRegisterVo"/>
        <where>  
            <if test="contractorName != null  and contractorName != ''"> and contractor_name like concat('%', #{contractorName}, '%')</if>
            <if test="personnelName != null  and personnelName != ''"> and personnel_name like concat('%', #{personnelName}, '%')</if>
            <if test="title != null  and title != ''"> and title = #{title}</if>
        </where>
    </select>
    
    <select id="selectBizPersonnelRegisterByPersonnelId" parameterType="Long" resultMap="BizPersonnelRegisterResult">
        <include refid="selectBizPersonnelRegisterVo"/>
        where personnel_id = #{personnelId}
    </select>

