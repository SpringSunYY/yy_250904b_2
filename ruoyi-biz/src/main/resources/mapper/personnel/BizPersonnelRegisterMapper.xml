    <update id="updateBizPersonnelRegister" parameterType="BizPersonnelRegister">
        update biz_personnel_register
        <trim prefix="SET" suffixOverrides=",">
            <if test="choiceId != null">choice_id = #{choiceId},</if>
            <if test="projectName != null">project_name = #{projectName},</if>
            <if test="contractorName != null">contractor_name = #{contractorName},</if>
            <if test="idCardNumber != null and idCardNumber != ''">id_card_number = #{idCardNumber},</if>
            <if test="personnelName != null">personnel_name = #{personnelName},</if>
            <if test="title != null">title = #{title},</if>
            <if test="gender != null">gender = #{gender},</if>
            <if test="age != null">age = #{age},</if>
            <if test="workType != null">work_type = #{workType},</if>
            <if test="contactPhone != null">contact_phone = #{contactPhone},</if>
            <if test="homeAddress != null">home_address = #{homeAddress},</if>
