        insert into biz_personnel_register
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="choiceId != null">choice_id,</if>
            <if test="projectName != null">project_name,</if>
            <if test="contractorName != null">contractor_name,</if>
            <if test="idCardNumber != null and idCardNumber != ''">id_card_number,</if>
            <if test="personnelName != null">personnel_name,</if>
            <if test="title != null">title,</if>
            <if test="gender != null">gender,</if>
            <if test="age != null">age,</if>
            <if test="workType != null">work_type,</if>
            <if test="contactPhone != null">contact_phone,</if>
            <if test="homeAddress != null">home_address,</if>
            <if test="physicalExamDate != null">physical_exam_date,</if>
            <if test="physicalExamResult != null">physical_exam_result,</if>
            <if test="physicalExamRecord != null">physical_exam_record,</if>
            <if test="entryFactoryTime != null">entry_factory_time,</if>
